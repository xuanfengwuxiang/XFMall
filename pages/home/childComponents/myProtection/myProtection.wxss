/* 首页 我的环保 样式 */

.my_protection {
  margin-top: 20rpx;
  margin-left: 10rpx;
}

.my_protection_tittle {
  display: flex;
  align-items: center;
}

.my_protection_content {
  margin-top: 10rpx;
  margin-left: 70rpx;
  margin-right: 70rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.my_protection_content_item {
  text-align: center;
  background-color: #fbe4a2;
  padding-left: 15rpx;
  padding-right: 15rpx;
}

.my_protection image {
  width: 60rpx;
  height: 60rpx;
}

.my_protection text {
  font-size: 20rpx;
}
