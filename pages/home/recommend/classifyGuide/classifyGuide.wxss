.logs {
  height: 100vh;
  width: 100vw;
  margin: 0;
  padding: 0;
  display: flex;
  flex-direction: column;
}

.container123 {
  margin-top: 20rpx;
  width: 100vw;

  display: flex;
}

.left {
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
}

.left_container {
  border-radius: 15rpx;
  background-color: #eee;
  width: 160rpx;
  height: 160rpx;
  margin-bottom: 5rpx;
  text-align: center;
  line-height: 160rpx;
}

.activityId {
  border-radius: 15rpx;
  line-height: 160rpx;
  color: burlywood;
  background-color: #eee;
  width: 160rpx;
  height: 160rpx;
  margin-bottom: 5rpx;
  text-align: center;
}
/* 顶部 */
.top{
  display: flex;
  flex-direction: row;
  width: 100vw;
  height: 80rpx;
  justify-content: center;
  align-items: center;
}
.select{
  flex: 1;
  font-weight: 450;
  font-size: 36rpx;
  text-align: center;
}
.normal{
  flex: 1;
  color: #ccc;
  font-weight: 450;
  font-size: 36rpx;
  text-align: center;
}